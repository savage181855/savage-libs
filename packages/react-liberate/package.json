{
	"name": "react-liberate",
	"version": "2.0.2",
	"description": "simple and elegant React Global State Manager",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"buildOptions": {
		"format": [
			"esm",
			"cjs",
			"umd"
		],
		"dts": true,
		"minify": true,
		"libraryName": "ReactLiberate",
		"globalVariableName": {
			"react": "React",
			"react-dom": "ReactDOM",
			"savage-types": "SavageTypes",
			"savage-utils": "SavageUtils"
		}
	},
	"unpkg": "./dist/index.umd.js",
	"jsdelivr": "./dist/index.umd.js",
	"type": "module",
	"scripts": {
		"test": "vitest"
	},
	"keywords": [
		"react-store",
		"react",
		"store",
		"redux",
		"redux-store"
	],
	"author": "",
	"license": "GPL-3.0",
	"dependencies": {
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"savage-types": "workspace:^",
		"savage-utils": "workspace:^1.0.58"
	},
	"peerDependencies": {
		"react": "^16.8.0",
		"react-dom": "^16.8.0"
	},
	"devDependencies": {
		"@types/react": "^16.8.0",
		"@types/react-dom": "^16.8.0",
		"vitest": "^0.34.3"
	}
}
