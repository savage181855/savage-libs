{
	"name": "esbuild-plugin-umd",
	"version": "0.0.0",
	"description": "A esbuild plugin for building UMD module",
	"miniprogram": "dist",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"buildOptions": {
		"format": [
			"esm",
			"cjs"
		],
		"dts": true,
		"minify": true
	},
	"type": "module",
	"scripts": {},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {},
	"dependencies": {}
}
