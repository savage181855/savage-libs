{
	"name": "savage-utils",
	"version": "1.0.54",
	"description": "js工具库",
	"miniprogram": "dist",
	"main": "index.js",
	"module": "dist/savage-utils.esm-bundler.js",
	"types": "dist/savage-utils.d.ts",
	"files": [
		"index.js",
		"dist"
	],
	"buildOptions": {
		"name": "savageUtils",
