{
	"name": "react-liberate-persist",
	"version": "0.0.11",
	"description": "a plugin of react-liberate that provide storage ability.",
	"miniprogram": "dist",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"buildOptions": {
		"format": [
			"esm",
			"cjs",
			"umd"
		],
		"dts": true,
		"minify": true,
		"libraryName": "ReactLiberatePersist",
		"globalVariableName": {},
		"external": []
	},
	"unpkg": "./dist/index.umd.js",
	"jsdelivr": "./dist/index.umd.js",
	"license": "GPL-3.0",
	"type": "module",
	"scripts": {},
	"keywords": [],
	"author": "",
	"dependencies": {
		"savage-types": "workspace:^",
		"savage-utils": "workspace:^1.0.65"
	},
	"devDependencies": {
		"react-liberate": "workspace:^",
		"vitest": "^0.34.3"
	}
}
