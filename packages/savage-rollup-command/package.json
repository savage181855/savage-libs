{
	"name": "savage-rollup-command",
	"version": "0.0.13",
	"description": "一个rollup插件，用于在rollup构建过程中执行自定义命令。",
	"miniprogram": "dist",
	"main": "index.js",
	"module": "dist/rollup-command.esm-bundler.js",
	"types": "dist/rollup-command.d.ts",
	"files": [
		"index.js",
		"dist"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "GPL-3.0",
	"homepage": "https://github.com/savage181855",
	"scripts": {
		"dev": "cross-env mode=dev nodemon rollup.config.js",
		"build": "cross-env mode=pro rollup -c rollup.config.js",
		"prepush": "npm run build",
		"push": "npm publish --access public",
		"lint": "eslint .",
		"commit": "git-cz"
	},
	"dependencies": {
		"cross-spawn": "7.0.3",
		"del": "^5.1.0"
	}
}
