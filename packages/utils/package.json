{
	"name": "savage-utils",
	"version": "1.0.50",
	"description": "js工具库",
	"main": "dist/index.cjs",
	"module": "./dist/index.mjs",
	"miniprogram": "dist",
	"files": [
		"dist/index.cjs",
		"dist/index.mjs",
		"dist/main.d.ts",
		"dist/index.cjs.map",
		"dist/index.mjs.map"
	],
	"types": "./dist/main.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"dev": "cross-env mode=dev nodemon rollup.config.js",
		"build": "cross-env mode=pro rollup -c ../../build/rollup.config.ts --configPlugin rollup-plugin-typescript2",
		"prepush": "npm run build",
		"push": "npm publish --access public",
		"lint": "eslint .",
		"commit": "git-cz"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"license": "GPL-3.0",
	"dependencies": {
		"savage-data-types": "workspace:*"
	}
}
