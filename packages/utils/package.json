{
	"name": "savage-utils",
	"version": "1.0.50",
	"description": "js工具库",
	"miniprogram": "dist",
  "main": "index.js",
  "module": "dist/shared.esm-bundler.js",
  "types": "dist/shared.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
	"buildOptions": {
    "name": "savage-utils",
    "formats": [
      "esm-bundler",
      "cjs"
    ]
  },
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"license": "GPL-3.0",
	"dependencies": {
		"savage-data-types": "workspace:*"
	}
}
