{
	"name": "savage-electron-ipc",
	"version": "0.0.12",
	"description": "一个用于electron的通信库",
	"miniprogram": "dist",
	"main": "index.js",
	"module": "dist/electron-ipc.esm-bundler.js",
	"types": "dist/electron-ipc.d.ts",
	"files": [
		"index.js",
		"dist"
	],
	"keywords": [
		"electron-ipc",
		"electrion",
		"ipc",
		"ipc-electron",
		"electron-publish",
		"electron-subscribe",
		"electron ipc",
		"ipc electron"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"dev": "cross-env mode=dev nodemon rollup.config.js",
		"build": "cross-env mode=pro rollup -c rollup.config.js",
		"prepush": "npm run build",
		"push": "npm publish --access public",
		"lint": "eslint .",
		"commit": "git-cz"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"license": "GPL-3.0",
	"devDependencies": {},
	"dependencies": {
		"electron": "^25.0.1"
	}
}
