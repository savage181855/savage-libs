{
	"name": "savage-rollup-command",
	"version": "0.0.10",
	"description": "一个rollup插件，用于在rollup构建过程中执行自定义命令。",
	"main": "dist/index.cjs",
	"module": "./dist/index.mjs",
	"files": [
		"dist/index.cjs",
		"dist/index.mjs",
		"dist/main.d.ts",
		"dist/index.cjs.map",
		"dist/index.mjs.map"
	],
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"require": "./dist/index.cjs"
		}
	},
	"type": "module",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "GPL-3.0",
	"homepage": "https://github.com/savage181855",
	"scripts": {
		"dev": "cross-env mode=dev nodemon rollup.config.js",
		"build": "cross-env mode=pro rollup -c rollup.config.js",
		"prepush": "npm run build",
		"push": "npm publish --access public",
		"lint": "eslint .",
		"commit": "git-cz"
	},
	"devDependencies": {},
	"dependencies": {
		"del": "^5.1.0"
	}
}
