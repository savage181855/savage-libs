{
	"name": "savage-types",
	"version": "1.0.13",
	"description": "一个用于判断js变量类型的库",
	"miniprogram": "dist",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"buildOptions": {
		"format": [
			"esm",
			"cjs"
		],
		"dts": true,
		"minify": true
	},
	"type": "module",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"generate": "tsx src/generate.ts"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"author": {
		"name": "savage",
		"email": "2443005004@qq.com",
		"url": "https://github.com/savage181855/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/savage181855/data-types"
	},
	"homepage": "https://github.com/savage181855/data-types",
	"bugs": {
		"url": "https://github.com/savage181855/data-types",
		"email": "2443005004@qq.com"
	},
	"license": "GPL-3.0",
	"dependencies": {
		"axios": "^1.5.0",
		"cheerio": "1.0.0-rc.12"
	}
}
